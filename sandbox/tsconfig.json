{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noUnusedLocals": true,
    "sourceMap": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "resolvePackageJsonExports": true,
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true,
    "jsxImportSource": "solid-js",
    "preserveSymlinks": true,
    "types": ["@types/node"]
  }
}
