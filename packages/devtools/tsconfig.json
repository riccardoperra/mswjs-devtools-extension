{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noUnusedLocals": true,
    "composite": false,
    "sourceMap": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "jsxImportSource": "solid-js",
    "types": ["@types/node"]
  },
  "include": ["./**/*", "../../env.d.ts", "env.d.ts"]
}
