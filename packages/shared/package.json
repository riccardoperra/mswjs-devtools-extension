  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "dependencies": {
    "@solid-primitives/event-bus": "^0.1.3",
    "@solid-primitives/event-listener": "^2.2.3",
    "@solid-primitives/media": "^2.0.3",
    "@solid-primitives/rootless": "^1.2.0",
    "@solid-primitives/scheduled": "^1.1.0",
    "@solid-primitives/styles": "^0.0.100",
